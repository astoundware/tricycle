{
  "name": "tricycle",
  "version": "2.6.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "macos": "msbuild macos/Tricycle.macOS.sln && open macos/Tricycle.UI.macOS/bin/Debug/Tricycle.app"
  },
  "dependencies": {
    "@react-native-picker/picker": "^2.4.1",
    "i18next": "^21.8.4",
    "react": "17.0.1",
    "react-i18next": "^11.16.9",
    "react-native": "0.64.3",
    "react-native-macos": "^0.64.0-0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "^\\.(.*)/constants/colors$": "<rootDir>/App/constants/colors.macos.js",
      "^\\.(.*)/images$": "<rootDir>/App/images/index.macos.js"
